<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".FakeGpsDataUploadForShowOffActivity">

    <Button
        android:id="@+id/uploadFakeDataButton"
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:background="@android:color/holo_blue_dark"
        android:text="start upload fake gps data"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23" />

    <Button
        android:id="@+id/ShowGarbageTruckOnMapButton"
        android:layout_width="197dp"
        android:layout_height="60dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Show Garbage Truck On Map"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75" />

    <Button
        android:id="@+id/stopUploadFakeDataButton"
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:background="@android:color/holo_blue_dark"
        android:text="stop Upload Fake Data"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>